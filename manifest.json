{
    "manifest_version": 3,
    "name": "FollowComp",
    "description": "Compare your following list to your followers list",
    "version": "0.1",
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["https://www.instagram.com/*"],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
      "service_worker": "scripts/background.js"
    }
}